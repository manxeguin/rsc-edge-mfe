  },
  "dependencies": {
    "react": "18.3.0-next-fecc288b7-20221025",
    "react-dom": "18.3.0-next-fecc288b7-20221025",
    "react-server-dom-webpack": "18.3.0-next-fecc288b7-20221025"
  },
  "packageManager": "npm@8.19.2"
}
